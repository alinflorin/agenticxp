    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "eventsource": "^3.0.7",
    "fastify": "^5.3.2",
    "fastify-type-provider-yup": "^5.0.0",
    "i18next": "^25.1.2",
    "i18next-browser-languagedetector": "^8.1.0",
    "i18next-http-backend": "^3.0.2",
