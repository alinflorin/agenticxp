{
  "name": "agenticxp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev-fe": "vite",
    "build-fe": "vite build",
    "build-be": "esbuild src/server.ts --bundle --platform=node --format=cjs --outfile=dist/server/server.cjs",
    "dev-be": "tsx watch src/server.ts",
    "dev": "concurrently \"npm run dev-fe\" \"npm run dev-be\"",
    "build": "concurrently \"npm run build-fe\" \"npm run build-be\""
  },
  "dependencies": {
    "@fastify/static": "^8.1.1",
    "dotenv": "^16.5.0",
    "fastify": "^5.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/dotenv": "^6.1.1",
    "@types/node": "^22.14.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "esbuild": "^0.25.2",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "tsx": "^4.19.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1"
  }
}
