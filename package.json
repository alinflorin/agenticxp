  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev-fe": "vite",
    "build-fe": "vite build",
    "build-be": "rimraf dist/server/** && esbuild src/_api/server.ts --bundle --platform=node --format=cjs --outfile=dist/server/server.cjs && cpy \"node_modules/@fastify/swagger-ui/static/**\" dist/server/swagger",
    "dev-be": "tsx watch src/_api/server.ts",
    "dev": "concurrently \"npm run dev-fe\" \"npm run dev-be\"",
    "build": "concurrently \"npm run build-fe\" \"npm run build-be\"",
    "tsx": "tsx"
  },
